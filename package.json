{
  "name": "formsync-nx",
  "version": "1.0.0",
  "description": "FormSync Monorepo with NX",
  "main": "index.js",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build",
    "test": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "serve": "nx run-many --target=serve --all",
    "serve:front": "nx serve front",
    "serve:backend": "nx serve backend",
    "deploy:all": "nx run-many --target=deploy --all",
    "deploy:infra": "nx run terraform:deploy",
    "deploy:frontend": "nx run front:deploy",
    "deploy:backend": "nx run backend:deploy",
    "deploy:extensions": "nx run-many --target=deploy --projects=chrome-extension,firefox-extension",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "reset": "nx reset"
  },
  "keywords": [
    "formsync",
    "nx",
    "monorepo",
    "angular",
    "spring-boot",
    "terraform"
  ],
  "author": "FormSync Team",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.4",
    "@angular/animations": "^20.3.3",
    "@angular/cli": "^20.3.4",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@nx/angular": "^21.6.3",
    "@nx/eslint": "^21.6.3",
    "@nx/jest": "^21.6.3",
    "@nx/js": "^21.6.3",
    "@nx/workspace": "^21.6.3",
    "@types/chart.js": "^2.9.41",
    "@types/jasmine": "^5.1.9",
    "@types/validator": "^13.15.3",
    "autoprefixer": "^10.4.21",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "nx": "^21.6.3",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@angular/cdk": "^20.2.7",
    "@angular/material": "^20.2.7",
    "brazilian-values": "^0.13.1",
    "chart.js": "^4.5.0",
    "ng2-charts": "^8.0.0",
    "ngx-mask": "^20.0.3",
    "rxjs": "^7.8.2",
    "validator": "^13.15.15",
    "zone.js": "^0.15.1"
  }
}
